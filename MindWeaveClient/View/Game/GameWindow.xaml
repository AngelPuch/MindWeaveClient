<Window x:Class="MindWeaveClient.View.Game.GameWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MindWeaveClient.View.Game"
        mc:Ignorable="d"
        Title="Mind Weave - In Game"
        Height="720" Width="1280">

    <Grid>
        <Grid.Background>
            <ImageBrush ImageSource="/Resources/Images/background.png" Stretch="UniformToFill"/>
        </Grid.Background>
        <Frame x:Name="GameFrame" NavigationUIVisibility="Hidden"/>
    </Grid>
</Window>