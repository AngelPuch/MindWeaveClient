<Window x:Class="MindWeaveClient.View.Authentication.AuthenticationWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MindWeaveClient.View.Authentication"
        mc:Ignorable="d"
        Title="Mind Weave" Height="720" Width="760">
    <Grid>
        <Grid.Background>
            <ImageBrush ImageSource="/Resources/Images/background.png" Stretch="UniformToFill"/>
        </Grid.Background>
        <Frame x:Name="AuthenticationFrame" NavigationUIVisibility="Hidden"/>
    </Grid>
</Window>
